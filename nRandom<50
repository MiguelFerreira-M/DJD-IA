class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();

            int n = r.Next(1,101);
            int nR = 0;

            Console.Write(n + " numeros \n");

            for (int i = 0; i < n; i++)
            {
                int nRandom = r.Next(1,101);
                if (nRandom > 50) nR++;
            }

            Console.Write(nR + " numeros acima de 50\n");
        }
    }
